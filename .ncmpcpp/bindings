def_key "/"
    find
def_key "/"
    find_item_forward

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-b"
    page_up
#push_characters "kkkkkkkkkkkkkkk"
def_key "ctrl-f"
    page_down
#push_characters "jjjjjjjjjjjjjjj"

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

